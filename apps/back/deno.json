{
  "name": "@app/back",
  "exports": "./src/index.ts",
  "imports": {
    "@hono/standard-validator": "npm:@hono/standard-validator@^0.1.2",
    "hono": "npm:hono@^4.6.20"
  },
  "tasks": {
    "dev": "deno run -A --unstable-otel --env-file=../../.env --env-file=../../.env.local main.ts",
