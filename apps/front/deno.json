{
  "name": "@app/front",
  "exports": "./src/index.ts",
  "tasks": {
    "dev": "deno run -A --node-modules-dir --env-file=../../.env --env-file=../../.env.local --env-file=../../.env.development --env-file=../../.env.development.local npm:vite",
    "build": "deno run -A --node-modules-dir --env-file=../../.env --env-file=../../.env.local --env-file=../../.env.production --env-file=../../.env.production.local npm:vite build",
    "preview": "deno run -A --env-file=../../.env --env-file=../../.env.local --env-file=../../.env.production --env-file=../../.env.production.local --node-modules-dir npm:vite preview",
    "serve": "deno run --allow-net --allow-read --allow-env dist/ --env-file=../../.env --env-file=../../.env.local --env-file=../../.env.production --env-file=../../.env.production.local jsr:@std/http@1/file-server"
  },
  "compilerOptions": {
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "jsxImportSourceTypes": "@types/react"
  },
  "imports": {
    "@deno/vite-plugin": "npm:@deno/vite-plugin@^1.0.0",
    "@types/react": "npm:@types/react@^18.3.12",
    "@types/react-dom": "npm:@types/react-dom@^18.3.1",
    "@vitejs/plugin-react": "npm:@vitejs/plugin-react@^4.3.4",
    "react": "npm:react@^18.3.1",
    "react-dom": "npm:react-dom@^18.3.1",
    "vite": "npm:vite@^6.0.1"
  }
}
